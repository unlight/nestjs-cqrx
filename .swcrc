{
    "sourceMaps": false,
    "module":
    {
        "type": "commonjs",
        "strict": true,
        "strictMode": true,
        "lazy": false,
        "noInterop": false
    },
    "jsc":
    {
        "keepClassNames": true,
        "loose": true,
        "externalHelpers": true,
        "target": "es2020",
        "parser":
        {
            "syntax": "typescript",
            "tsx": false,
            "decorators": true
        },
        "transform":
        {
            "legacyDecorator": true,
            "decoratorMetadata": true
        }
    }
}
