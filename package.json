{
  "name": "nestjs-cqrx",
  "version": "0.0.0-dev",
  "license": "MIT",
  "description": "",
  "keywords": [
    "cqrs",
    "nestjs",
    "eventstore",
    "eventstored"
  ],
  "scripts": {
    "test": "npm run eslint && npm run tscheck && npm run test:cov",
    "test:p": "ultra test",
    "test:r": "jest --runInBand --verbose",
    "test:cov": "jest --runInBand --coverage",
    "test:w": "npm run test:r -- --watch",
    "lint": "npm run eslint && npm run tscheck",
    "eslint": "node node_modules/eslint/bin/eslint \"src/**/*.{ts,tsx}\"",
    "eslint:w": "watchexec -w src \"npm run eslint\"",
    "eslint:fix": "npm run eslint -- --fix",
    "tscheck": "tsc --noEmit",
    "tscheck:w": "npm run tscheck -- --watch",
    "lint:fix": "npm run eslint:fix && npm run lint:doc:fix",
    "format:src": "prettier src --write",
    "commit_lint": "sh Taskfile commit_lint",
    "commit": "cz",
    "build": "sh Taskfile build",
    "lint:w": "run-p tscheck:w eslint:w",
    "example": "node-dev -r tsconfig-paths/register example/main.ts",
    "example:debug": "ndb node-dev -r ts-node/register/transpile-only -r tsconfig-paths/register example/main.ts",
    "example:spec": "npm run test:r -- --verbose --rootDir=example"
  },
  "ultra": {
    "concurrent": [
      "lint",
      "test"
    ]
  },
  "devDependencies": {
    "@abraham/reflection": "^0.8.0",
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@eventstore/db-client": "^2.1.0",
    "@nestjs/common": "^8.0.6",
    "@nestjs/core": "^8.0.6",
    "@nestjs/cqrs": "^8.0.0",
    "@nestjs/platform-express": "^8.0.6",
    "@semantic-release/changelog": "^6.0.0",
    "@semantic-release/git": "^10.0.0",
    "@types/jest": "^27.0.2",
    "@types/lodash": "^4.14.173",
    "@types/node": "^16.9.6",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "commitizen": "^4.2.4",
    "conventional-changelog-conventionalcommits": "^4.6.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-plugin-etc": "^1.5.4",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-only-warn": "^1.0.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-regexp": "^1.2.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sonarjs": "^0.10.0",
    "eslint-plugin-sort-class-members": "^1.11.0",
    "eslint-plugin-total-functions": "^4.10.1",
    "eslint-plugin-unicorn": "^36.0.0",
    "eslint-plugin-wix-editor": "^3.3.0",
    "expect": "^27.2.1",
    "git-branch-is": "^4.0.0",
    "it-all": "^1.0.5",
    "jest": "^27.2.1",
    "jest-mock-extended": "^2.0.4",
    "lodash": "^4.17.21",
    "node-dev": "^7.0.0",
    "npm-run-all": "^4.1.5",
    "precise-commits": "^1.0.2",
    "prettier": "^2.4.1",
    "semantic-release": "^18.0.0",
    "simplytyped": "^3.3.0",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.5",
    "tsconfig-paths": "^3.11.0",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3",
    "ultra-runner": "^3.10.5",
    "watchexec-bin": "^1.0.0"
  },
  "dependencies": {
    "erroz": "^1.1.0"
  }
}
