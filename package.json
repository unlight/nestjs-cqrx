{
  "name": "nestjs-cqrx",
  "version": "0.0.0-dev",
  "license": "MIT",
  "description": "CQRS for NestJS",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unlight/nestjs-cqrx.git"
  },
  "bugs": {
    "url": "https://github.com/unlight/nestjs-cqrx/issues"
  },
  "homepage": "https://github.com/unlight/nestjs-cqrx#readme",
  "keywords": [
    "cqrs",
    "nestjs",
    "eventstore",
    "eventstoredb"
  ],
  "scripts": {
    "test": "npm run eslint && npm run tscheck && npm run test:cov",
    "test:p": "ultra test",
    "test:r": "jest --runInBand --roots=src",
    "test:d": "ndb npm run test:w -- --testTimeout=0",
    "test:cov": "jest --runInBand --coverage --roots=src",
    "test:w": "npm run test:r -- --watch",
    "lint": "npm run eslint && npm run tscheck",
    "eslint": "node node_modules/eslint/bin/eslint \"src/**/*.{ts,tsx}\"",
    "eslint:w": "watchexec -w src \"npm run eslint\"",
    "eslint:fix": "npm run eslint -- --fix",
    "tscheck": "tsc --noEmit",
    "tscheck:w": "npm run tscheck -- --watch",
    "lint:fix": "npm run eslint:fix && npm run lint:doc:fix",
    "format:src": "prettier src --write",
    "commit_lint": "sh Taskfile commit_lint",
    "commit": "cz",
    "build": "sh Taskfile build_tsup",
    "lint:w": "run-p tscheck:w eslint:w",
    "example": "node-dev -r tsconfig-paths/register example/main.ts",
    "example-tick-tack-toe-cqrx": "node-dev -r tsconfig-paths/register example-tick-tack-toe-cqrx/main.ts",
    "example:test:w": "jest --runInBand --roots=example --watch",
    "example:test:d": "ndb node node_modules/jest-cli/bin/jest --runInBand --roots=example --watch",
    "example:debug": "ndb node-dev -r ts-node/register/transpile-only -r tsconfig-paths/register example/main.ts",
    "example:spec": "jest --runInBand --roots=example",
    "tttx:test:w": "jest --runInBand --roots=example-tick-tack-toe-cqrx --watch",
    "mocha": "node -r tsconfig-paths/register -r @swc/register node_modules/mocha/bin/_mocha",
    "tttx:mocha:r": "npm run mocha -- example-tick-tack-toe-cqrx/**/*.spec.ts",
    "tttx:mocha:w": "npm run tttx:mocha:r -- --watch-files src,example-tick-tack-toe-cqrx --watch"
  },
  "ultra": {
    "concurrent": [
      "lint",
      "test"
    ]
  },
  "dependencies": {
    "node-is-class": "^1.0.4"
  },
  "peerDependencies": {
    "@eventstore/db-client": ">=3",
    "@nestjs/common": ">=8",
    "@nestjs/core": ">=8",
    "@nestjs/cqrs": ">=8",
    "rxjs": ">=7"
  },
  "devDependencies": {
    "@apollo/gateway": "^0.48.1",
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@nestjs/common": "^8.3.1",
    "@nestjs/core": "^8.3.1",
    "@nestjs/cqrs": "^8.0.2",
    "@nestjs/graphql": "10.0.6",
    "@nestjs/platform-express": "^8.4.0",
    "@nestjs/testing": "^8.4.0",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@swc/cli": "^0.1.55",
    "@swc/core": "1.2.148",
    "@swc/helpers": "0.3.6",
    "@swc/jest": "0.2.20",
    "@swc/register": "^0.1.10",
    "@types/jest": "27.4.1",
    "@types/lodash": "^4.14.179",
    "@types/mocha": "^9.1.0",
    "@types/node": "17.0.21",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "@ultimate-backend/event-store": "^0.13.5",
    "apollo-server-core": "^3.6.3",
    "apollo-server-express": "^3.6.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "commitizen": "^4.2.4",
    "conventional-changelog-conventionalcommits": "^4.6.3",
    "cuid": "^2.1.8",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-plugin-etc": "^2.0.2",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-only-warn": "^1.0.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-regexp": "^1.5.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sort-class-members": "^1.14.1",
    "eslint-plugin-unicorn": "^41.0.0",
    "eslint-plugin-wix-editor": "^3.3.0",
    "event-sourcing-generator": "^2.1.1",
    "expect": "^27.5.1",
    "extensible-custom-error": "^0.0.7",
    "git-branch-is": "^4.0.0",
    "graphql": "^16.3.0",
    "it-all": "^1.0.6",
    "jest": "^27.5.1",
    "jest-mock-extended": "^2.0.4",
    "lodash": "^4.17.21",
    "mocha": "^9.2.1",
    "node-dev": "^7.2.0",
    "npm-run-all": "^4.1.5",
    "precise-commits": "^1.0.2",
    "prettier": "^2.5.1",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.2",
    "rxjs": "^7.5.4",
    "semantic-release": "^19.0.2",
    "serialize-error": "8.X",
    "simplytyped": "^3.3.0",
    "supertest": "^6.2.2",
    "ts-node": "^10.6.0",
    "tsconfig-paths": "^3.13.0",
    "tslib": "^2.3.1",
    "typescript": "^4.6.2",
    "ultra-runner": "^3.10.5",
    "watchexec-bin": "^1.0.0"
  }
}
