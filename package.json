{
  "name": "nestjs-cqrx",
  "version": "0.0.0-dev",
  "license": "MIT",
  "description": "CQRS for NestJS",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unlight/nestjs-cqrx.git"
  },
  "bugs": {
    "url": "https://github.com/unlight/nestjs-cqrx/issues"
  },
  "homepage": "https://github.com/unlight/nestjs-cqrx#readme",
  "keywords": [
    "cqrs",
    "nestjs",
    "eventstore",
    "eventstoredb"
  ],
  "scripts": {
    "test": "npm run eslint && npm run tscheck && npm run test:r",
    "test:r": "npm run mocha -- src/**/*.spec.ts",
    "test:w": "npm run test:r -- --watch --watch-files src",
    "lint": "npm run eslint && npm run tscheck",
    "eslint": "node node_modules/eslint/bin/eslint \"src/**/*.{ts,tsx}\"",
    "eslint:w": "watchexec -w src \"npm run eslint\"",
    "eslint:fix": "npm run eslint -- --fix",
    "tscheck": "tsc --noEmit",
    "tscheck:w": "npm run tscheck -- --watch",
    "lint:fix": "npm run eslint:fix && npm run lint:doc:fix",
    "format:src": "prettier src --write",
    "commit_lint": "sh Taskfile commit_lint",
    "commit": "cz",
    "build": "sh Taskfile build_tsup",
    "lint:w": "run-p tscheck:w eslint:w",
    "example": "node-dev -r tsconfig-paths/register example/main.ts",
    "example-tick-tack-toe-cqrx": "node-dev -r tsconfig-paths/register example-tick-tack-toe-cqrx/main.ts",
    "example:test:w": "npm run mocha -- example/**/*.spec.ts --watch --watch-files example,src",
    "example:spec": "npm run mocha -- example/**/*.spec",
    "tttx:test:w": "npm run mocha -- example-tick-tack-toe-cqrx/**/*.spec.ts --watch",
    "mocha": "node -r tsconfig-paths/register -r ts-node/register node_modules/mocha/bin/_mocha --timeout=5s --ui=mocha-jested",
    "tttx:mocha:r": "npm run mocha -- example-tick-tack-toe-cqrx/**/*.spec.ts",
    "tttx:mocha:w": "npm run tttx:mocha:r -- --watch-files src,example-tick-tack-toe-cqrx --watch"
  },
  "dependencies": {
    "node-is-class": "^1.0.4"
  },
  "peerDependencies": {
    "@eventstore/db-client": "^6",
    "@nestjs/common": "^10",
    "@nestjs/core": "^10",
    "@nestjs/cqrs": "^10",
    "rxjs": "^7"
  },
  "devDependencies": {
    "@apollo/gateway": "^2.7.2",
    "@commitlint/cli": "^19.2.1",
    "@commitlint/config-conventional": "^19.1.0",
    "@eventstore/db-client": "^6.1.0",
    "@nestjs/common": "^10.3.0",
    "@nestjs/core": "^10.3.0",
    "@nestjs/cqrs": "^10.2.0",
    "@nestjs/graphql": "^12.1.1",
    "@nestjs/platform-express": "^10.3.5",
    "@nestjs/testing": "^10.3.5",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@swc/cli": "^0.3.10",
    "@swc/core": "^1.4.8",
    "@swc/helpers": "^0.5.7",
    "@swc/register": "^0.1.10",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.11.30",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "apollo-server-core": "^3.13.0",
    "apollo-server-express": "^3.13.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "commitizen": "^4.3.0",
    "conventional-changelog-conventionalcommits": "^7.0.2",
    "cuid": "^2.1.8",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-plugin-etc": "^2.0.3",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-only-warn": "^1.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-regexp": "^2.4.0",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "eslint-plugin-sort-class-members": "^1.20.0",
    "eslint-plugin-unicorn": "^51.0.1",
    "eslint-plugin-wix-editor": "^3.3.0",
    "event-sourcing-generator": "^2.1.1",
    "expect": "^29.7.0",
    "extensible-custom-error": "^0.0.7",
    "git-branch-is": "^4.0.0",
    "graphql": "^16.8.1",
    "it-all": "1.X",
    "lodash": "^4.17.21",
    "mocha": "^10.3.0",
    "mocha-jested": "^2.4.0",
    "node-dev": "^8.0.0",
    "npm-run-all": "^4.1.5",
    "precise-commits": "^1.0.2",
    "prettier": "^3.2.5",
    "reflect-metadata": "^0.1.12",
    "request": "^2.88.2",
    "rxjs": "^7.5.4",
    "semantic-release": "^23.0.5",
    "serialize-error": "8.X",
    "simplytyped": "^3.3.0",
    "supertest": "^6.3.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.6.2",
    "typescript": "^5.4.3",
    "watchexec-bin": "^1.0.0"
  }
}
