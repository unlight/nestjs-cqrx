{
  "name": "nestjs-cqrx",
  "private": true,
  "version": "0.0.0-dev",
  "license": "MIT",
  "description": "CQRS for NestJS",
  "type": "commonjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "workspaces": ["apps/tick-tack-toe", "apps/user"],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unlight/nestjs-cqrx.git"
  },
  "bugs": {
    "url": "https://github.com/unlight/nestjs-cqrx/issues"
  },
  "homepage": "https://github.com/unlight/nestjs-cqrx#readme",
  "keywords": ["cqrs", "nestjs", "eventstore", "eventstoredb"],
  "scripts": {
    "test": "npm run eslint && npm run tscheck && npm run test:r",
    "test:r": "ts-node node_modules/mocha/bin/mocha.js src/**/*.spec.ts",
    "test:w": "ts-node node_modules/mocha/bin/mocha.js --no-timeouts --watch-files src/**/*.ts --watch src/**/*.spec.ts",
    "lint": "npm run eslint && npm run tscheck",
    "eslint": "eslint \"src/**/*.{ts,tsx}\"",
    "eslint:w": "onchange -v \"src/**\" -k -i -- \"npm run eslint\"",
    "tt2": "onchange -v \"src/**\" -k -i -- \"npm run test:r\"",
    "eslint:fix": "npm run eslint -- --fix",
    "tscheck": "tsc --noEmit",
    "tscheck:w": "npm run tscheck -- --watch",
    "format:src": "prettier src --write",
    "build": "sh Taskfile buildMicrobundle5",
    "lint:w": "run-p tscheck:w eslint:w",
    "dev:user": "onchange -v **/.ts -k -i -- npx ts-node apps/user/main.ts",
    "dev:ttt": "onchange -v **/.ts -k -i -- npx ts-node apps/tick-tack-toe/main.ts",
    "test:user:w": "npm run test:user:r -- --watch --watch-files src --watch-files apps/user",
    "test:user:r": "npm run mocha -- apps/user/**/*.spec.ts",
    "test:ttt:r": "npm run mocha -- apps/tick-tack-toe/**/*.spec.ts",
    "test:ttt:w": "npm run test:ttt:r -- --watch --watch-files apps/tick-tack-toe,src",
    "mocha": "node -r ts-node/register node_modules/mocha/bin/mocha.js --no-timeouts"
  },
  "dependencies": {
    "node-is-class": "^1.0.4"
  },
  "peerDependencies": {
    "@kurrent/kurrentdb-client": "1 - 1",
    "@nestjs/common": "8 - 11",
    "@nestjs/core": "8 - 11",
    "@nestjs/cqrs": "8 - 11",
    "rxjs": "6 - 7"
  },
  "devDependencies": {
    "@apollo/gateway": "^2.11.2",
    "@eslint/compat": "^1.3.2",
    "@eslint/js": "^9.35.0",
    "@kurrent/kurrentdb-client": "^1.0.4",
    "@nestjs/common": "10",
    "@nestjs/core": "10",
    "@nestjs/cqrs": "10",
    "@nestjs/graphql": "^13.1.0",
    "@nestjs/platform-express": "^11.1.6",
    "@nestjs/testing": "^11.1.6",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@swc/cli": "^0.7.8",
    "@swc/core": "^1.13.5",
    "@swc/helpers": "^0.5.17",
    "@swc/register": "^0.1.10",
    "@types/lodash": "^4.17.20",
    "@types/mocha": "^10.0.10",
    "@types/node": "^24.3.1",
    "@types/supertest": "^6.0.3",
    "apollo-server-express": "^3.13.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cuid": "^2.1.8",
    "dts-bundle-generator": "^9.5.1",
    "eslint": "^9.35.0",
    "conventional-changelog-conventionalcommits": "^9.1.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-only-warn": "^1.1.0",
    "eslint-plugin-prettier": "^5.5.4",
    "expect": "^30.1.2",
    "glob": "^11.0.3",
    "globals": "^16.3.0",
    "graphql": "^16.11.0",
    "it-all": "1.X",
    "lodash": "^4.17.21",
    "microbundle": "^0.15.1",
    "mocha": "^11.7.2",
    "node-dev": "^8.0.0",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.1.0",
    "precise-commits": "^1.0.2",
    "prettier": "^3.6.2",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "semantic-release": "^24.2.7",
    "serialize-error": "11.X",
    "simplytyped": "^3.3.0",
    "supertest": "^7.1.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.42.0"
  }
}
