{
  "name": "nestjs-cqrx",
  "private": true,
  "version": "0.0.0-dev",
  "license": "MIT",
  "description": "CQRS for NestJS",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "workspaces": [
    "apps/tick-tack-toe",
    "apps/user"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unlight/nestjs-cqrx.git"
  },
  "bugs": {
    "url": "https://github.com/unlight/nestjs-cqrx/issues"
  },
  "homepage": "https://github.com/unlight/nestjs-cqrx#readme",
  "keywords": [
    "cqrs",
    "nestjs",
    "eventstore",
    "eventstoredb"
  ],
  "scripts": {
    "test": "npm run eslint && npm run tscheck && npm run test:r",
    "test:r": "npm run mocha -- src/**/*.spec.ts",
    "test:w": "npm run test:r -- --watch --watch-files src",
    "lint": "npm run eslint && npm run tscheck",
    "eslint": "node node_modules/eslint/bin/eslint \"src/**/*.{ts,tsx}\"",
    "eslint:w": "watchexec -w src \"npm run eslint\"",
    "eslint:fix": "npm run eslint -- --fix",
    "tscheck": "tsc --noEmit",
    "tscheck:w": "npm run tscheck -- --watch",
    "lint:fix": "npm run eslint:fix && npm run lint:doc:fix",
    "format:src": "prettier src --write",
    "commit_lint": "sh Taskfile commit_lint",
    "commit": "cz",
    "build": "sh Taskfile build_tsup",
    "lint:w": "run-p tscheck:w eslint:w",
    "dev:user": "node-dev -r tsconfig-paths/register apps/user/main.ts",
    "dev:ttt": "node-dev -r tsconfig-paths/register apps/tick-tack-toe/main.ts",
    "test:user:w": "npm run test:user:r -- --watch --watch-files src --watch-files apps/user",
    "test:user:r": "npm run mocha -- apps/user/**/*.spec.ts",
    "test:ttt:r": "npm run mocha -- apps/tick-tack-toe/**/*.spec.ts",
    "test:ttt:w": "npm run test:ttt:r -- --watch --watch-files apps/tick-tack-toe,src",
    "mocha": "node -r tsconfig-paths/register -r ts-node/register node_modules/mocha/bin/_mocha --no-timeouts --ui=mocha-jested"
  },
  "dependencies": {
    "node-is-class": "^1.0.4"
  },
  "peerDependencies": {
    "@eventstore/db-client": "6",
    "@nestjs/common": "8 - 11",
    "@nestjs/core": "8 - 11",
    "@nestjs/cqrs": "8 - 11",
    "rxjs": "6 - 7"
  },
  "devDependencies": {
    "@apollo/gateway": "^2.9.3",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@eslint/compat": "^1.2.1",
    "@eslint/js": "^9.13.0",
    "@eventstore/db-client": "^6.1.0",
    "@nestjs/common": "10",
    "@nestjs/core": "10",
    "@nestjs/cqrs": "10",
    "@nestjs/graphql": "^12.2.0",
    "@nestjs/platform-express": "^10.4.4",
    "@nestjs/testing": "^10.4.4",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@swc/cli": "^0.4.0",
    "@swc/core": "^1.7.26",
    "@swc/helpers": "^0.5.13",
    "@swc/register": "^0.1.10",
    "@types/jest": "^29.5.13",
    "@types/lodash": "^4.17.10",
    "@types/mocha": "^10.0.8",
    "@types/node": "^22.7.4",
    "@types/supertest": "^6.0.2",
    "apollo-server-core": "^3.13.0",
    "apollo-server-express": "^3.13.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "commitizen": "^4.3.1",
    "conventional-changelog-conventionalcommits": "^8.0.0",
    "cuid": "^2.1.8",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-only-warn": "^1.1.0",
    "eslint-plugin-perfectionist": "^3.8.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unicorn": "^56.0.0",
    "eslint-plugin-wix-editor": "^3.3.0",
    "expect": "^29.7.0",
    "globals": "^15.10.0",
    "graphql": "^16.9.0",
    "it-all": "1.X",
    "jest-mock": "^29.7.0",
    "lodash": "^4.17.21",
    "mocha": "^10.7.3",
    "mocha-jested": "^2.4.1",
    "node-dev": "^8.0.0",
    "npm-run-all": "^4.1.5",
    "precise-commits": "^1.0.2",
    "prettier": "^3.3.3",
    "reflect-metadata": "^0.2.2",
    "request": "^2.88.2",
    "rxjs": "7",
    "semantic-release": "^24.1.2",
    "serialize-error": "11",
    "simplytyped": "^3.3.0",
    "supertest": "^7.0.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsup": "^8.3.0",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.8.0",
    "watchexec-bin": "^1.0.0"
  }
}
